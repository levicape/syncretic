{
  "name": "iac_stacks",
  "config": {
    "packagemanager": "pnpm",
    "packageexecutor": "pnpm exec",
    "lintpaths": "./src ./canary ./iac"
  },
  "dependencies": {
    "@levicape/fourtwo": "workspace:",
    "@levicape/fourtwo-pulumi": "workspace:",
    "@pulumi/aws": "catalog:pulumi",
    "@pulumi/awsx": "catalog:pulumi",
    "@pulumi/pulumi": "catalog:pulumi"
  },
  "devDependencies": {
    "@types/node": "catalog:types",
    "typescript": "catalog:dev"
  },
  "nx": {
    "targets": {
      "nx:compile": {
        "dependsOn": [
          "@levicape/fourtwo-pulumi:nx:compile",
          "iac_stacks_code:nx:compile"
        ]
      },
      "dx:precommit": {
        "dependsOn": [
          "nx:compile",
          "iac_stacks_bootstrap:nx:precommit",
          "iac_stacks_code:nx:precommit"
        ]
      }
    }
  },
  "scripts": {
    "compile": "$npm_package_config_packageexecutor nx nx:compile"
  }
}
